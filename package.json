{
  "name": "mammutmap",
  "productName": "mammutmap",
  "version": "1.9.1",
  "description": "TODO",
  "main": "dist/electronApp/index.js",
  "scripts": {
    "test": "jest --config ./test/jest.config.js",
    "testE2e": "npm run buildAppForOlderElectron && jest --config ./testE2e/jest.config.js --runInBand",
    "copyStaticFiles": "copyfiles -u 1 src/**/*.html src/**/*.css dist",
    "buildApp": "tsc && npm run copyStaticFiles",
    "buildAppForOlderElectron": "tsc --module commonjs && npm run copyStaticFiles",
    "buildPlugins": "cd plugin-src && tsc && cd ../ && node --disable-warning=ExperimentalWarning scripts/replacePluginSrcWithDistImports.mts",
    "buildPluginsForOlderElectron": "cd plugin-src && tsc --module commonjs && cd ../ && node --disable-warning=ExperimentalWarning scripts/replacePluginSrcWithDistImports.mts",
    "startWeb": "npm run buildApp && npm run buildPlugins && http-server ../ --ext js -c no-cache",
    "start": "npm run buildAppForOlderElectron && npm run buildPluginsForOlderElectron && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": "michael",
  "license": "AGPLv3",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ts_visualize_files"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@fontsource/source-code-pro": "^4.5.11",
    "@fontsource/source-sans-pro": "^4.5.11",
    "@types/jest": "^28.1.6",
    "@types/jest-image-snapshot": "^5.1.0",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "copyfiles": "^2.4.1",
    "electron": "^28.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^28.1.3",
    "jest-image-snapshot": "^5.1.0",
    "jest-mock-extended": "^3.0.4",
    "langchain": "0.0.125",
    "puppeteer-core": "^19.9.0",
    "sharp": "^0.33.2",
    "ts-jest": "^28.0.8",
    "typescript": "^5.2.2",
    "yarn": "^1.22.21"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "json5": "^2.2.3",
    "simple-git": "^3.22.0"
  }
}
